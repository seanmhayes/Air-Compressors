@article{Schein2006,
abstract = {Air handling unit performance assessment rules (APAR) is a fault detection tool that uses a set of expert rules derived from mass and energy balances to detect faults in air handling units (AHUs). Control signals are used to determine the mode of operation of the AHU. A subset of the expert rules which correspond to that mode of operation are then evaluated to determine whether a fault exists. APAR is computationally simple enough that it can be embedded in commercial building automation and control systems and relies only upon the sensor data and control signals that are commonly available in these systems. APAR was tested using data sets collected from a “hardware-in-the-loop” emulator and from several field sites. APAR was also embedded in commercial AHU controllers and tested in the emulator.},
author = {Schein, Jeffrey and Bushby, Steven T. and Castro, Natascha S. and House, John M.},
file = {:C$\backslash$:/Users/seanadmin/Documents/Mendeley Desktop/Schein et al/Energy and Buildings/Schein et al. - 2006 - A rule-based fault detection method for air handling units(2).pdf:pdf},
journal = {Energy and Buildings},
keywords = {buildingautomation,diagnostics,directdigital control,energymanagementsystem,fault detection},
number = {12},
pages = {1485--1492},
title = {{A rule-based fault detection method for air handling units}},
volume = {38},
year = {2006}
}
