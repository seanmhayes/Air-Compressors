isbn = {0306-2619},
issn = {03062619},
journal = {Applied Energy},
keywords = {Compressors,Efficiency,Gas turbine performance,HayesSM,Neural Networks,Neural network inverse,Optimisation,Optimization},
mendeley-tags = {Compressors,Efficiency,HayesSM,Neural Networks,Optimisation},
