isbn = {9780874216561},
journal = {Igarss 2014},
keywords = {high resolution images,research,risks management,sustainable reconstruction},
number = {1},
pages = {1--5},
title = {{APPLICATION OF A CAUSAL DIGRAPH BASED FAULT DIAGNOSIS METHOD WITH DISCRETE STATE SPACE MODEL ON A PAPER MACHINE SIMULATOR}},
