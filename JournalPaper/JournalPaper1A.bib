@incollection{Tassou2006,
abstract = {Refrigeration is part of everyday life and can be found in the home, car, office and industry. It is used very widely for food preservation, thermal comfort, food and drinks processing and chemical manufacturing. Since the inception of mechanical refrigeration towards the end of the 19th century two principal refrigeration cycles have emerged, the vapour compression and the vapour absorption cycle. The former cycle has dominated domestic and commercial applications due to its higher efficiency, smaller size and lower cost for the same refrigeration capacity. In recent years, environmental impacts from the working fluids of vapour compression systems, energy consumption and materials used in the construction of refrigeration equipment have increased pressure on the refrigeration industry to reduce the impacts of refrigeration systems through better design and control. This chapter presents results of investigations into the application of artificial intelligence techniques, based on Artificial Neural Networks, for the utilisation of data already collected from many large supermarkets for energy prediction, performance diagnosis and control. The results demonstrate that artificial intelligence can be used effectively to reduce both the energy consumption of refrigeration systems and predict deterioration in system performance due to gradual refrigerant leakage. © 2006 by Nova Science Publishers, Inc. All rights reserved.},
author = {Tassou, Savvas A. and Datta, Debarati and Grace, Ian N.},
booktitle = {Artificial Intelligence in Energy and Renewable Energy Systems},
isbn = {1600212611},
keywords = {Chillers,Expert Systems,Neural Networks},
mendeley-tags = {Chillers,Expert Systems,Neural Networks},
pages = {439--456},
publisher = {Nova Science Publishers, Inc.},
title = {{Application of artificial intelligence techniques for the reduction of energy consumption of refrigeration systems}},
url = {http://www.scopus.com/inward/record.url?eid=2-s2.0-84895232069{\&}partnerID=tZOtx3y1},
year = {2006}
}
