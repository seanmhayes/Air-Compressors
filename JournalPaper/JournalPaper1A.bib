journal = {International Journal of Thermodynamics; Vol 11, No 3: September 2008},
keywords = {diagnosis,fuzzy expert system,multi-objective evolutionary algorithms},
number = {3},
pages = {115--121},
title = {{Energy System Diagnosis by a Fuzzy Expert System with Genetically Evolved Rules}},
url = {http://www.ijoticat.com/index.php/IJoT/article/view/219/203},
volume = {11},
