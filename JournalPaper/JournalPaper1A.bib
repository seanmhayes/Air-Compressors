@article{Palau1999,
abstract = {This works focuses on using neural networks and expert systems to control a gas/solid sorption chilling machine. In such systems, the cold production changes cyclically with time due to the batchwise operation of the gas/solid reactors. The accurate simulation of the dynamic performance of the chilling machine has proven to be difficult for standard computers when using deterministic models. Additionally, some model parameters dynamically change with the reaction advancement. A new modelling approach is presented here to simulate the performance of such systems using neural networks. The backpropagation learning rule and the sigmoid transfer function have been applied in feedforward, full connected, single hidden layer neural networks. Overall control of this system is divided in three blocks: control of the machine stages, prediction of the machine performance and fault diagnosis. Ce travail traite de l'usage des r{\'{e}}seaux neuronaux et des syst{\`{e}}mes experts pour r{\'{e}}guler une machine frigorifique {\`{a}} sorption gaz/solide. Dans ce type de syst{\`{e}}me, la production de froid varie dans le temps de fa{\c{c}}on cyclique, du fait du fonctionnement discontinu des r{\'{e}}acteurs gaz/solide. La simulation pr{\'{e}}cise de la performance dynamique de la machine frigorifique s'est r{\'{e}}v{\'{e}}l{\'{e}}e difficile pour un ordinateur normal en cas d'utilisation de mod{\`{e}}les d{\'{e}}terministes. De plus, certains param{\`{e}}tres du mod{\`{e}}le varient au fur et {\`{a}} mesure de la r{\'{e}}action. On pr{\'{e}}sente ici une nouvelle approche de mod{\'{e}}lisation pour simuler la performance de tels syst{\`{e}}mes utilisant les r{\'{e}}seaux neuronaux. On applique la r{\'{e}}gle d'apprentissage par r{\'{e}}tropropagation et la fonction de transfert sigmoı̈de dans des r{\'{e}}seaux neuronaux {\`{a}} commande pr{\'{e}}dictive, enti{\`{e}}rement connect{\'{e}}s {\`{a}} couche cach{\'{e}}e simple. Le syst{\`{e}}me est divis{\'{e}} en trois parties: r{\'{e}}gulation des {\'{e}}tages des machines, pr{\'{e}}vision de la performance de la machine et diagnostic des d{\'{e}}fauts.},
author = {Palau, A and Velo, E and Puigjaner, L},
doi = {10.1016/S0140-7007(97)00046-7},
file = {:C$\backslash$:/Users/seanadmin/Documents/Mendeley Desktop/Palau, Velo, Puigjaner/International Journal of Refrigeration/Palau, Velo, Puigjaner - 1999 - Use of neural networks and expert systems to control a gassolid sorption chilling machine.pdf:pdf},
